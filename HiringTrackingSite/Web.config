	    <add name="Session" type="Microsoft.AspNet.SessionState.SessionStateModuleAsync, Microsoft.AspNet.SessionState.SessionStateModule, Version=2.0.0.0, Culture=neutral" preCondition="integratedMode,managedHandler" />
		
    </modules>
    <validation validateIntegratedModeConfiguration="false" />
  </system.webServer>
